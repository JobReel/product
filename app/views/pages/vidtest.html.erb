<div class = "js container is-flex nopadding">
 <div id = "video_container">
   <video controls preload poster="http://res.cloudinary.com/jobreel/video/upload/default_intro.jpg">
     <source src="http://res.cloudinary.com/jobreel/video/upload/default_intro.webm" type="video/webm">
     <source src="http://res.cloudinary.com/jobreel/video/upload/default_intro.mp4" type="video/mp4">
     <source src="http://res.cloudinary.com/jobreel/video/upload/default_intro.ogv" type="video/ogg">
   </video>
 </div>
 <div id = "videoControls">
   <button id = "play" title = "Play"> &#x25BA;</button>

   <div class = "progress-box vert-align" id = "progress">
     <div class = "progress-bar vert-align" id = "progress_box">
       <span class = "moving-bar" id = "play_progress"></span>
     </div>
   </div>

    <span id="curtimetext">0:00</span> / <span id="durtimetext">0:00</span>
    <input id="volumeslider" type="range" min="0" max="100" value="100" step="1" class="vranger">
 </div>

</div>
