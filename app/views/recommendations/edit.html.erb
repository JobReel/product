<div class="booyah-box col-6 offset-3">
  <h1>Edit Your User Information</h1>
  <%= simple_form_for @recommendations do |f| %>
    <%= f.input :company_name %>
    <%= f.input :company_title %>
    <%= f.input :time_known %>
    <%= f.input :user_relationship %>
    <%= f.input :profile_image %>
    <br />
    <%= f.submit 'Edit', class: 'btn btn-primary' %>
  <% end %>
</div>
